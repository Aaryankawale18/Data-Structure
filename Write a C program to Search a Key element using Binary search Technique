#include<stdio.h>
void main() {
	int a[20], i, j, n, key, flag = 0, low, high, mid, temp;
	printf("Enter value of n : ");
	scanf("%d", &n);
	for(i = 0; i < n;i ++){
		printf("Enter element for a[%d] : ", i);
		scanf("%d", &a[i]);
	}
	for(i = 0; i < n - 1; i++){
		for(j = 0; j < n - 1; j++){
			if(a[j] > a[j + 1]) {
				temp = a[j];
				a[j] = a[j + 1];
				a[j + 1] = temp;
			}
		}
	}

	printf("Enter key element : ");
}
